{
  "name": "bs-pmc-2023-team25",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "react-scripts test --transformIgnorePatterns \"/node_modules/(?!.*\\\\.jsx?$)\"",
    "start": "node server.js",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BS-PMC-2023/BS-PMC-2023-Team25.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BS-PMC-2023/BS-PMC-2023-Team25/issues"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "mongodb": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.10",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "eslint": "^8.41.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "extends": ["google", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": "warn",
    "no-unused-vars": "error",
    "no-undef": "error",
    "react/jsx-indent": ["error", 2],
    "react/jsx-quotes": ["error", "double"],
    "react/jsx-props-no-spreading": "off",
    "react/prop-types": "warn"
  }

}
